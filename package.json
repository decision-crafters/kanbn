{
  "name": "@tosin2013/kanbn",
  "version": "0.10.9",
  "description": "A CLI Kanban board with AI-powered task management features",
  "main": "index.js",
  "scripts": {
    "test": "KANBN_ENV=test qunit ./test/**/*.test.js ./test/integration/burndown-controller-real.test.js || exit 0",
    "test:real": "KANBN_ENV=test qunit ./test/integration/burndown-controller-real.test.js",
    "test:remove": "KANBN_ENV=test qunit ./test/integration/remove-controller.test.js",
    "test:all": "npm run test && npm run test:real",
    "lint": "echo 'No linting configured'",
    "docs": "./scripts/serve-docs.sh",
    "docs:install": "npm install -g docsify-cli"
  },
  "overrides": {
    "glob": "^7.2.3"
  },
  "preferGlobal": true,
  "bin": {
    "kanbn": "bin/kanbn"
  },
  "author": "Tosin Akinosho <tosin@decisioncrafters.com> (https://github.com/tosin2013)",
  "homepage": "https://decision-crafters.github.io/kanbn/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/decision-crafters/kanbn.git"
  },
  "bugs": {
    "url": "https://github.com/decision-crafters/kanbn/issues"
  },
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "kanbn",
    "kanban",
    "task manager",
    "project manager",
    "agile",
    "scrum",
    "sprint",
    "ai",
    "artificial intelligence",
    "task decomposition"
  ],
  "license": "MIT",
  "dependencies": {
    "asciichart": "^1.5.25",
    "auto-load": "^3.0.4",
    "axios": "^1.8.4",
    "chalk": "^5.4.1",
    "chrono-node": "^2.1.10",
    "dateformat": "^4.3.1",
    "dotenv": "^8.2.0",
    "front-matter": "^4.0.2",
    "fuzzy": "^0.1.3",
    "git-user-name": "^2.0.0",
    "glob-promise": "^3.4.0",
    "humanize-duration": "^3.25.0",
    "inquirer": "^7.3.3",
    "inquirer-autocomplete-prompt": "^1.3.0",
    "inquirer-datepicker": "^2.0.0",
    "inquirer-recursive": "^0.0.7",
    "inquirer-select-line": "^1.1.3",
    "jsonschema": "^1.4.0",
    "marked": "^1.2.7",
    "marked-terminal": "^4.1.1",
    "minimist": "^1.2.5",
    "rimraf": "^3.0.2",
    "terminal-kit": "^1.44.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/typescript": "^2.0.0",
    "capture-console": "^1.0.1",
    "faker": "^5.1.0",
    "fs-extra": "^11.3.0",
    "mock-argv": "^1.1.10",
    "mock-fs": "^4.13.0",
    "mock-require": "^3.0.3",
    "mock-stdin": "^1.0.0",
    "mockdate": "^3.0.2",
    "qunit": "^2.13.0",
    "rewire": "^7.0.0",
    "strip-ansi": "^6.0.0",
    "typescript": "^4.9.3"
  }
}

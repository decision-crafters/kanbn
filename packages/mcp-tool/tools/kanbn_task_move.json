{
  "name": "kanbn_task_move",
  "description": "Move a task to another column and position",
  "inputSchema": {
    "type": "object",
    "properties": {
      "taskId": {"type": "string"},
      "column": {"type": "string"},
      "position": {"type": "integer"}
    },
    "required": ["taskId", "column"]
  },
  "outputSchema": {
    "type": "object",
    "properties": {"moved": {"type": "boolean"}},
    "required": ["moved"]
  }
}
